<fest:template xmlns:fest="http://fest.mail.ru" context_name="json">
    <div class="row">
        <div class="col-lg-4 col-lg-offset-4 col-sm-4 col-sm-offset-4">
            <div class="table-responsive">
    			<table class="table ">
    				<tr class="tableHeader">
    					<th>Username</th>
    					<th>Scores</th>
    				</tr>
                    <fest:for iterate="json" index="i" value="v">
    					<tr class="clientRow">
    					    <td><fest:value>v.name</fest:value></td>
    						<td><fest:value>v.score</fest:value></td>
    					</tr>
                    </fest:for>
                </table>
            </div>
        </div>
    </div>
</fest:template>
